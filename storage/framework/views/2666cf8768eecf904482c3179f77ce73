        <script type="text/javascript">
            var maxPos = -5000;
            var pos = (Math.floor(Math.random()*-3000));

            if (Math.round(Math.random()) == 1) {
            	jQuery(".companies_scroller2 .logos_all").animate({marginLeft: pos}, 2500);
            } else {
            	jQuery(".companies_scroller2 .logos_all").animate({marginLeft: -5000}, 0);
            	jQuery(".companies_scroller2 .logos_all").animate({marginLeft: pos}, 2500);
            }

            jQuery(".companies_scroller2 .scroll_right").click(function() {
                if (pos > -5000) {
                    pos -= 500;
                    jQuery(".companies_scroller2 .logos_all").animate({marginLeft: '-=500'}, 1000);
                }              });
            jQuery(".companies_scroller2 .scroll_left").click(function() {
                if (pos < 0) {
                    pos += 500;
                    jQuery(".companies_scroller2 .logos_all").animate({marginLeft: '+=500'}, 1000);
                }
            });
        </script>
